{
  "name": "Twilio Voice Extension",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Twilio Voice Extension",
    "default_popup": "popup.html"
  }
}
